<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!--  <properties resource="jdbc.properties" />
         -->
    <!--location 数据库驱动jar包的绝对路径-->
    <classPathEntry
            location="G:/java/jar/repository/mysql/mysql-connector-java/8.0.22/mysql-connector-java-8.0.22.jar"/>
    <!--配置 table 表信息内容体, targetRuntime 指定采用 MyBatis 的版本-->
    <context id="msqlTables" targetRuntime="MyBatis3">
        <!--抑制生成注释(英文)-->
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!--配置数据库连接信息-->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://192.168.72.132:3306/web_user"
                        userId="admin"
                        password="123456">
            <!--解决mysql8.0以后重复生成所有表的问题,原因为表名称格式不符合标准-->
            <!--<property name="nullCatalogMeansCurrent" value="true" />-->
        </jdbcConnection>
        <!--生成 domain , targetPackage 指定目标包名,
        targetProject 指定生成的 domain 放在哪个项目下
        此处为相对路径 表示 当前目录的 src/main/java 下的目标包, 可以自动创建包-->
        <javaModelGenerator targetPackage="com.example.domain"
                            targetProject="src/main/java">
            <property name="enableSubPackages" value="false"/>
            <property name="trimStrings" value="false"/>
        </javaModelGenerator>
        <!--生成 mapper.xml-->
        <sqlMapGenerator targetPackage="com.example.mapperX"
                         targetProject="src/main/java">
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>
        <!--生成 mapper 接口-->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.example.mapper"
                             targetProject="src/main/java">
            <property name="enableSubPackages" value="false"/>
        </javaClientGenerator>
        <!--数据库表名以及对应的 java domain类 名, 有几个表就要写几个-->
        <table tableName="tb_address" domainObjectName="Address"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"/>
        <table tableName="tb_areas" domainObjectName="Areas"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"/>
        <table tableName="tb_cities" domainObjectName="Cities"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"/>
        <table tableName="tb_provinces" domainObjectName="Provinces"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"/>
        <table tableName="tb_user" domainObjectName="User"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"/>

    </context>
</generatorConfiguration>